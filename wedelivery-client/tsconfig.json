{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "nuxt",
      "node"  // Adicione 'node' aqui
    ]
  },
  "include": [
    "nuxt.config.ts",
    "cypress/**/*.ts",
    "components/**/*.vue"
  ]
}
